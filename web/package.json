{
  "name": "sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../server && modd",
    "backend": "AUDITLOG=users,posts ../server/entrypoint.sh ./pocketbase serve --publicDir=../web/build",
    "build": "vite build",
    "build:backend": "cd ../server && go build",
    "typegen": "pocketbase-typegen --db ../server/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.15",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "sass": "^1.77.5",
    "svelte": "^5.0.0-next.155",
    "svelte-check": "^3.8.0",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "tailwindcss": "^3.4.4",
    "postcss": "^8.4.38",
    "autoprefixer": "^10.4.19",
    "@tailwindcss/typography": "^0.5.13",
    "prettier-plugin-tailwindcss": "^0.6.4"
  },
