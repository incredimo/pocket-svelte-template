    "cmdk-sv": "^0.0.17",
    "embla-carousel-svelte": "^8.1.5",
    "formsnap": "^1.0.0",
    "lucide-svelte": "^0.395.0",
    "mode-watcher": "^0.3.0",
    "paneforge": "^0.0.4",
    "pocketbase": "^0.21.3",
    "svelte-sonner": "^0.3.24",
    "sveltekit-superforms": "^2.15.1",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
